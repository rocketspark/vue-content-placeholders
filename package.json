{
  "name": "vue-content-placeholders",
  "description": "Vue Components for rendering animated content placeholders like facebook (Fork of https://github.com/michalsnik/vue-content-placeholders)",
  "version": "1.0.0",
  "author": "Rocketspark (http://github.com/rocketspark)",
  "keywords": [
    "vue",
    "vuejs",
    "plugin",
    "loader",
    "fake",
    "placeholder",
    "progressive",
    "skeleton-ui"
  ],
  "license": "MIT",
  "main": "dist/vue-content-placeholders.common.js",
  "unpkg": "dist/vue-content-placeholders.browser.js",
  "module": "index.js",
  "scripts": {
    "dev": "NODE_ENV=development webpack --config config/webpack.config.dev.js --progress --watch",
    "build": "npm run build:browser && npm run build:common",
    "build:browser": "NODE_ENV=production webpack --config config/webpack.config.browser.js --progress --hide-modules",
    "build:common": "NODE_ENV=production webpack --config config/webpack.config.common.js --progress --hide-modules",
    "prepublish": "npm run build",
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/michalsnik/vue-content-placeholders.git"
  },
  "bugs": {
    "url": "https://github.com/michalsnik/vue-content-placeholders/issues"
  },
  "homepage": "https://github.com/michalsnik/vue-content-placeholders#readme",
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/eslint-parser": "^7.25.1",
    "@babel/preset-env": "^7.25.3",
    "@vue/compiler-sfc": "^3.4.37",
    "babel-loader": "^9.1.3",
    "babel-preset-env": "^1.6.1",
    "css-loader": "^7.1.2",
    "eslint": "^9.9.0",
    "eslint-plugin-html": "^8.1.1",
    "eslint-plugin-vue": "^9.27.0",
    "eslint-webpack-plugin": "^4.2.0",
    "mini-css-extract-plugin": "^2.9.0",
    "neostandard": "^0.11.2",
    "sass": "^1.77.8",
    "sass-loader": "^16.0.0",
    "vue": "^3.4.37",
    "vue-loader": "^17.4.2",
    "vue-template-compiler": "^2.1.6",
    "webpack": "^5.93.0",
    "webpack-cli": "^5.1.4",
    "webpack-merge": "^6.0.1"
  }
}
